{
  {% for tier, cars in tiers.items() %}
  "{{tier.name.title()}}": [
    {% for car_info in cars %}
    {
      {% for name, value in car_info.row.items() %}
      "{{name.value}}": "{{value}}"{{"" if loop.last else ","}}
      {% endfor %}
    }{{"" if loop.last else ","}}
    {% endfor %}
  ]{{"" if loop.last else ","}}
  {% endfor %}
}
